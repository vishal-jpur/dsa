//frequency of each alphabet in a string

#include<bits/stdc++.h>
using namespace std;
int main()
{
   string s="iamsogoodatart";
   map<char,int> m;
   
   for(int i=0;i<s.size();i++)                                             
   {
      m[s[i]]++;
   }
   
   for(auto i:m)
   {
      cout<<i.first<<" "<<i.second<<endl;
   }
}

a 3
d 1
g 1
i 1
m 1
o 3
r 1
s 1
t 2
